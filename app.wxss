.Tfixed{
  height: 88rpx;
  line-height: 88rpx;
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
}
.Tfixed view{
  width: 33%;
  float: left;
  text-align: center;
}
.Tfixed view:nth-of-type(1){
  background: #e6e6e6;
  color: #5a5a5a;
  font-size: 28rpx
}
