/* pages/selectAgency/selectAgency.wxss */
.created{
    background: #fff;
}

.tle{
  text-indent: 32rpx;
    height: 40rpx;
    line-height:40rpx;
    font-size: 28rpx;
    color:#5a5a5a;
    padding: 32rpx;
    background: #fff;
    border-bottom:1rpx solid #ebebeb; 
    position: relative
}
.tle::after{
  position: absolute;
  content: '';
  width: 8rpx;
  height: 40rpx;
  background: #ff7f6c;
  left: 32rpx;
  top: 32rpx;
  border-radius: 4rpx;
}
.enters{
  position: absolute;
  width: 28rpx!important;
  height: 28rpx!important;
  top: 50%;
  margin-top: -14rpx;
  right: 32rpx;
}
.box{
    margin-bottom: 8rpx;
}
.info.cell-space {
justify-content: flex-start;
}
.boxItem{
    padding: 32rpx;
    border-bottom: 1rpx solid #ebebeb;
    overflow: hidden;
    background: #fff;
    height:96rpx;
    position: relative
}

.boxItem image{
    float: left;
    vertical-align: bottom;
    height: 96rpx;
    width: 96rpx;
    border-radius: 48rpx;
    margin-right: 20rpx;
}

.boxItem .Ssize{
  margin-left: 32rpx;
    color: #fff!important;
    background: #ff7f6c;
    border-radius: 8rpx;
    /* padding: 5rpx 10rpx; */
    padding: 0 10rpx;
    height: 36rpx;
    line-height: 36rpx;
    justify-content: flex-start!important
}

.tab{
    color: #77C6FF!important;
    border:1rpx solid #77C6FF;
    border-radius: 8rpx;
    /* padding: 5rpx 10rpx; */
    padding: 0 10rpx;
    height: 36rpx;
    line-height: 36rpx;
    position: absolute;
    top: 50%;
    right: 32rpx;
    margin-top: -18rpx;
    font-size: 24rpx;
}

.add.boxItem{
    line-height: 100rpx;
    height: 100rpx;
    margin-bottom: 8rpx
}